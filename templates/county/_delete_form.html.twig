<form method="post" action="{{ path('county_delete', {'id': county.id}) }}" class="text-center" onsubmit="return confirm('Are you sure you want to delete this item?');" class="ml-1">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ county.id) }}">
    <button class="btn btn-outline-danger"><i class="fas fa-trash"></i></button>
</form>
